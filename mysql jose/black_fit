----TABLA CLIENTE----
create table if not exists clientes (
    Id_cliente int not null auto_increment,
    Nombre varchar(25),
    Apellido varchar(25),
    Genero varchar(20),
    Fecha_nac date,
    Fecha_ingreso date,
    Fecha_fin date,
    primary key (id_cliente)
    );

