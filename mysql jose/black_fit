----TABLA DE MANTENIMIENTO----
create table if not exists mantenimiento (
    Id_mantenimineto int not null auto_increment,
	Tipo_mantenimiento varchar(40),
	Tiempo_meses int,
	primary key (Id_mantenimineto)
	);

